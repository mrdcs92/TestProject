<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  <context-param>
  <param-name>Team</param-name>
  <param-value>Mountaineers</param-value>
  </context-param>
  
  <servlet>
  	<servlet-name>CoolServlet</servlet-name>
  	<display-name>CoolServlet</display-name>
  	<description></description>
  	<servlet-class>serve.CoolServlet</servlet-class>
  	
  	<init-param>
  	<param-name>Quarterback</param-name>
  	<param-value>Will Grier</param-value>
  	</init-param>
  	
  	<load-on-startup>
  	12
  	</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>MyServlet</servlet-name>
  	<display-name>MyServlet</display-name>
  	<description></description>
  	<servlet-class>serve.MyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>CoolServlet</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>MyServlet</servlet-name>
  	<url-pattern>/MyServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
